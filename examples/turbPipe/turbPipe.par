#[OCCA]
#backend = CUDA
#deviceNumber = LOCAL-RANK

[GENERAL]
#verbose = true
polynomialOrder = 7
#startFrom = "restart.fld"
stopAt = endTime
endTime = 200

dt = 6.0e-3
#dt = targetCFL=4 + max=1e-2 + initial=6e-3

timeStepper = tombo2
subCyclingSteps = 2

writeControl = simulationTime 
writeInterval = 1

regularization = hpfrt + nModes=1 + scalingCoeff=10

[PRESSURE]
residualTol = 1e-04
#initialGuess = projectionAconj+nVector=10

[VELOCITY]
#solver = pfcg+block
boundaryTypeMap = inlet, outlet, wall
density = 1.0
viscosity = 1/19000
residualTol = 1e-06

[CASEDATA]
#/* diameters  */
ZLENGTH = 20.0
NSLABS  = 60
NELSLAB = 108
