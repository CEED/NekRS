[GENERAL] 
polynomialOrder = 9
#startFrom = "restart.fld"
stopAt = endTime
endTime = 50 

dt = 7e-3
timeStepper = tombo2
subCyclingSteps = 1

writeControl = simulationTime 
writeInterval = 1.0

regularization = hpfrt + nModes = 1 + scalingCoeff = 8

[PRESSURE]
residualTol = 1e-04

[VELOCITY]
boundaryTypeMap = inlet, outlet, wall, slipY, slipZ
residualTol = 1e-06
density = 1.0
viscosity = -1400
